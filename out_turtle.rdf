@prefix ns1: <https://poneyponeymastersaga/myontology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://poneyponeymastersaga/myontology> a <http://www.w3.org/2002/07/owl#Ontology> .

ns1:age a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:patient ;
    rdfs:range xsd:integer .

ns1:date a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:consultation ;
    rdfs:range xsd:dateTime,
        xsd:string .

ns1:dayra a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:patient ;
    rdfs:range xsd:string .

ns1:familyName a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:person ;
    rdfs:range xsd:string .

ns1:familyname a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:person ;
    rdfs:range xsd:string .

ns1:gender a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:person ;
    rdfs:range xsd:string .

ns1:givenName a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:person ;
    rdfs:range xsd:string .

ns1:hadConsultation a <http://www.w3.org/2002/07/owl#ObjectProperty> ;
    rdfs:domain ns1:patient ;
    rdfs:range ns1:consultation .

ns1:hasChronicDesease a <http://www.w3.org/2002/07/owl#ObjectProperty> ;
    rdfs:domain ns1:patient ;
    rdfs:range ns1:chronicDesease .

ns1:hasPatient a <http://www.w3.org/2002/07/owl#FunctionalProperty>,
        <http://www.w3.org/2002/07/owl#ObjectProperty> ;
    rdfs:domain ns1:consultation ;
    rdfs:range ns1:patient .

ns1:hasSymptom a <http://www.w3.org/2002/07/owl#ObjectProperty> ;
    rdfs:domain ns1:patient ;
    rdfs:range ns1:symptoms .

ns1:output a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:consultation ;
    rdfs:range xsd:string .

ns1:pat a <http://www.w3.org/2002/07/owl#FunctionalProperty>,
        <http://www.w3.org/2002/07/owl#ObjectProperty> ;
    rdfs:domain ns1:consultation ;
    rdfs:range ns1:patient .

ns1:symptomsPresented a <http://www.w3.org/2002/07/owl#ObjectProperty> ;
    rdfs:domain ns1:consultation ;
    rdfs:range ns1:symptoms .

ns1:wilaya a <http://www.w3.org/2002/07/owl#DatatypeProperty>,
        <http://www.w3.org/2002/07/owl#FunctionalProperty> ;
    rdfs:domain ns1:patient ;
    rdfs:range xsd:string .

ns1:chronicDesease a <http://www.w3.org/2002/07/owl#Class> ;
    rdfs:subClassOf <http://www.w3.org/2002/07/owl#Thing> .

ns1:consultation1 a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:consultation ;
    ns1:date "2020-06-28"^^xsd:string ;
    ns1:hasPatient ns1:patient1 ;
    ns1:symptomsPresented ns1:fever,
        ns1:khra,
        ns1:shit .

ns1:consultation2 a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:consultation ;
    ns1:date "2020-06-28"^^xsd:string ;
    ns1:hasPatient ns1:patient1 ;
    ns1:symptomsPresented ns1:headache .

ns1:consultation3 a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:consultation ;
    ns1:date "2020-06-28"^^xsd:string ;
    ns1:hasPatient ns1:patient2 ;
    ns1:symptomsPresented ns1:fever .

ns1:khra a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:symptoms .

ns1:patient2 a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:patient ;
    ns1:age "10"^^xsd:string ;
    ns1:dayra "Douera"^^xsd:string ;
    ns1:familyName "Harkat"^^xsd:string ;
    ns1:gender "male"^^xsd:string ;
    ns1:givenName "Adel Larbi"^^xsd:string ;
    ns1:hadConsultation ns1:consultation3 ;
    ns1:hasSymptom ns1:fever ;
    ns1:wilaya "Alger"^^xsd:string .

ns1:shit a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:symptoms .

ns1:headache a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:symptoms .

ns1:patient1 a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:patient ;
    ns1:age "21"^^xsd:string ;
    ns1:dayra "Douera"^^xsd:string ;
    ns1:familyName "Ihadjadene"^^xsd:string ;
    ns1:gender "male"^^xsd:string ;
    ns1:givenName "Abdelhak"^^xsd:string ;
    ns1:hadConsultation ns1:consultation1,
        ns1:consultation2 ;
    ns1:hasSymptom ns1:headache ;
    ns1:wilaya "Alger"^^xsd:string .

ns1:fever a <http://www.w3.org/2002/07/owl#NamedIndividual>,
        ns1:symptoms .

ns1:person a <http://www.w3.org/2002/07/owl#Class> ;
    rdfs:subClassOf <http://www.w3.org/2002/07/owl#Thing> .

ns1:symptoms a <http://www.w3.org/2002/07/owl#Class> ;
    rdfs:subClassOf <http://www.w3.org/2002/07/owl#Thing> .

ns1:consultation a <http://www.w3.org/2002/07/owl#Class> ;
    rdfs:subClassOf <http://www.w3.org/2002/07/owl#Thing> .

ns1:patient a <http://www.w3.org/2002/07/owl#Class> ;
    rdfs:subClassOf ns1:person .

